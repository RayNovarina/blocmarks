<% # Calc current values for rpt_bmk(props)[:first_column?], etc.
   # Access via rpt_bmk_helpers[:fields...]
  rpt_bmk_calc_helpers(props)
%>
  <% if rpt_bmk(props)[:container] == :rows %>
    <%= render partial: '/bookmarks/container_styles/rows/rows_container',
               locals: { props: props }
    %>
  <% elsif rpt_bmk(props)[:container] == :carousel %>
    <%= render partial: '/bookmarks/container_styles/carousel/carousel_container',
               locals: { props: props }
    %>
  <% end %>
