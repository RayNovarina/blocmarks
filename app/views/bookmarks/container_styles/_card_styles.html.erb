<% if rpt_bmk(props)[:style] == :columns %>
  <%= render partial: '/bookmarks/card_styles/columns/columns_bmk',
             locals: { props: props }
  %>
<% elsif rpt_bmk(props)[:style] == :embedly %>
  <%= render partial: '/bookmarks/card_styles/embedly/embedly_bmk',
             locals: { props: props }
  %>
<% elsif rpt_bmk(props)[:style] == :embed_ray %>
  <%= render partial: '/bookmarks/card_styles/embed_ray/embed_ray_bmk',
             locals: { props: props }
  %>
<% elsif rpt_bmk(props)[:style] == :embed_side_by_side %>
  <%= render partial: '/bookmarks/card_styles/embed_side_by_side/embed_side_by_side_bmk',
             locals: { props: props }
  %>
<% end %>
