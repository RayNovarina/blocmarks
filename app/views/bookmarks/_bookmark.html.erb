<% # Calc current values for rpt_bmk(props)[:first_column?], etc.
   # Access via rpt_bmk_helpers[:fields...]
  rpt_bmk_calc_helpers(props)
%>
<% if rpt_bmk(props)[:style] == :columns %>
  <%= render partial: '/bookmarks/styles/columns/columns_bmk',
             locals: { props: props }
  %>
<% elsif rpt_bmk(props)[:style] == :embedly %>
  <%= render partial: '/bookmarks/styles/embedly/embedly_bmk',
             locals: { props: props }
  %>
<% elsif rpt_bmk(props)[:style] == :embed_ray %>
  <%= render partial: '/bookmarks/styles/embed_ray/embed_ray_bmk',
             locals: { props: props }
  %>
<% elsif rpt_bmk(props)[:style] == :embed_side_by_side %>
  <%= render partial: '/bookmarks/styles/embed_side_by_side/embed_side_by_side_bmk',
             locals: { props: props }
  %>
<% elsif rpt_bmk(props)[:style] == :carousel %>
  <%= render partial: '/bookmarks/styles/carousel/carousel_bmk',
             locals: { props: props }
  %>
<% end %>
